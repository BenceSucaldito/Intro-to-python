x = int(input(""))
y = int(input(""))
if x == 1 and y%2 > 0:
    print("Yes")
elif x == 2 and y%2 == 0:
    print("Yes")
elif x == 3 and y%2 > 0:
    print("Yes")
elif x == 4 and y%2 == 0:
    print("Yes")
elif x == 5 and y%2 > 0:
    print("Yes")
elif x == 6 and y%2 == 0:
    print("Yes")
elif x == 7 and y%2 > 0:
    print("Yes")
elif x == 8 and y%2 == 0:
    print("Yes")
else:
    print("No")
