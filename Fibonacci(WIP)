term1 = int(input("Enter first term: "))
term2 = int(input("Enter second term:"))
term3 = 0
max_count = int(input("Enter how many terms you want"))
print("Term 1: {}\nTerm 2: {}".format(term1,term2))
for i in range(3,max_count+1):
    term3 = term1 + term2
    print("Term {}: {}".format(i, term3))
    term1 = term2
    term2 = term3
    term3 = 0
